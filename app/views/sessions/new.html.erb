<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <h1><center>Log in</h1>
    <%= form_for(:session, url: login_path) do |f| %>
      <%= f.label :first_Name %>
      <%= f.text_field :first_name, class: 'form-control' %><br>
      <%= f.label :phone_Number %>
      <%= f.text_field :phone_number, class: 'form-control' %><br>
      <center><%= f.submit "Log in", class: "btn btn-primary btn-lg btn-block" %></center>
    <% end %>
    <b>Don't have an account?<u><%= link_to 'Create an account', new_user_path, :class => "btn btn-secondary" %></u>  </b>
  </div>
</div>
